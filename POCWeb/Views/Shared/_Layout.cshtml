<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/vue/css/bootstrap-vue.min.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <img src="~/img/logo.png" class="img-thumbnail">
                <a class="navbar-brand" asp-area="" asp-page="/Index">POCWeb</a>
            </div>
        </nav>
    </header>

    <div class="container" id="app">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
        </div>
    </footer>

    <script src="~/lib/vue/js/vue.js"></script>
    <script src="~/lib/vue/js/bootstrap-vue.min.js"></script>
    <script src="~/lib/vue/js/vee-validate.full.min.js"></script>
    <script src="~/lib/vue/js/axios.min.js"></script>
    <script src="~/lib/vue/js/vue-qrcode/vue-qrcode.js"></script>
    <script src="~/js/app.js"></script>
</body>
</html>
